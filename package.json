{
  "name": "fullstack-dev-challenge",
  "version": "0.0.1",
  "private": true,
  "dependencies": {
    "@types/chai": "^4.3.0",
    "@types/jest": "^27.4.0",
    "@types/mocha": "^9.0.0",
    "@types/supertest": "^2.0.11",
    "chai": "^4.3.4",
    "cors": "^2.8.5",
    "express": "4.17.1",
    "nodemon": "^2.0.15",
    "supertest": "^6.1.6"
  },
  "scripts": {
    "start": "concurrently \"npm run server\" \"npm run client\"",
    "server": "nodemon --watch './**/*.ts' --exec 'ts-node' server.ts",
    "types": "tsc",
    "client": "node scripts/start-client.js",
    "install": "cd client && yarn install && cd ..",
    "test": "mocha -r ts-node/register ./test/*.spec.ts"
  },
  "devDependencies": {
    "@types/cors": "^2.8.9",
    "@types/express": "^4.17.9",
    "@types/node": "^14.14.14",
    "concurrently": "5.3.0",
    "mocha": "^8.2.1",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.3"
  }
}
